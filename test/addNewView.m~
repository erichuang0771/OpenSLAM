function [ pc, newfeatureTable ] = addNewView( ims, featureTable )
%UNTITLED2 Summary of this function goes here
%   featureTable SIFTdes|3D
% SIFTdes = 128 * N

      [f,d] = vl_sift(single(rgb2gray(ims)));
      [matches, scores] = vl_ubcmatch(d, featureTable(:,1:128)');
      P1 = [f(1,matches(1,:))' f(2,matches(1,:))', ones(size(matches(1,:)))'];
      XX = featureTable(129:end)

     
    


end

